<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <!-- Tribe -->

  <RulePackDef>
    <defName>NamerFactionSlugVillage</defName>
    <rulePack>
      <rulesStrings>
        <li>name->The [negative] [terrainfeature] [SlugNames]</li>
        <li>name->[color] [badAdj] [SlugNames2] </li>
        <li>name->[negative] [badAdj] [terrainfeature]</li>
        <li>name->The [slugAdj] [SlugNames] [SlugNames2]</li>
        <li>name->[negative] [color] [SlugNames2] [terrainfeature]</li>
		<li>name->[slugAdj] [negative] [politicalunion]</li>
		<li>name->[slugAdj] [terrainfeature] [politicalunion]</li>
		<li>name->[slugAdj] [SlugNames2] [politicalunion]</li>
		<li>name->[badAdj] [SlugNames] [politicalunion]</li>
      </rulesStrings>
      <rulesRaw>
		<li Class="Rule_File">
          <keyword>politicalunion</keyword>
          <path>Words/Nouns/PoliticalUnions_Outlander</path>
        </li>
        <li Class="Rule_File">
          <keyword>terrainfeature</keyword>
          <path>Words/Nouns/TerrainFeatures</path>
        </li>
		<!-- adj -->
