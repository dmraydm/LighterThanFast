<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <!-- Tribe -->

  <RulePackDef>
    <defName>NamerFactionRockmanVillage</defName>
    <rulePack>
      <rulesStrings>
        <li>name->The [color] [minerals] [mountains]</li>
        <li>name->The [positive] mines of the [color] [mountains]</li>
        <li>name->[positive] [minerals] [politicalunion]</li>
        <li>name->[color] [minerals] [terrainfeature]</li>
        <li>name->[color] [mountains] [politicalunion] </li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>mountains</keyword>
          <path>RockmanNames/Mountains</path>
        </li>
        <li Class="Rule_File">
          <keyword>minerals</keyword>
          <path>RockmanNames/Minerals</path>
        </li>
        <li Class="Rule_File">
          <keyword>color</keyword>
          <path>Words/Nouns/Colors</path>
        </li>
		<li Class="Rule_File">
          <keyword>positive</keyword>
          <path>Words/Nouns/Concepts_Positive</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerBaseRockmanVillage</defName>
    <rulePack>
      <rulesStrings>
        <li>name->The [color] [minerals] [mountains]</li>
        <li>name->[positive] [terrainfeature] of the [color] [mountains]</li>
        <li>name->[positive] [minerals] [politicalunion]</li>
        <li>name->[color] [minerals] [terrainfeature]</li>
        <li>name->[positive] [terrainfeature] [politicalunion] </li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>mountains</keyword>
          <path>RockmanNames/Mountains</path>
        </li>
        <li Class="Rule_File">
          <keyword>minerals</keyword>
          <path>RockmanNames/Minerals</path>
        </li>
        <li Class="Rule_File">
          <keyword>color</keyword>
          <path>Words/Nouns/Colors</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

</Defs>
