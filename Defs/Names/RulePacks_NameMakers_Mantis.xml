<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerPersonMantis</defName>
    <rulePack>
		<rulesStrings>
        <li>name->[Locations] '[badass]' [tribalword]</li>
        <li>name->[Locations] '[Adjective]' [Gangsta]</li>
		<li>name->[Locations] '[Races]' [Gangsta]</li>
		<li>name->[Locations] '[Adjective]' [MantisNames]</li>
		
		<li>name->[Races] '[Locations]' [badassanimal]</li>
		
		
        <li>name->[tribalword] '[color]' [MantisNames]</li>
		<li>name->[tribalword] '[Adjective]' [MantisNames]</li>
		<li>name->[tribalword] '[Races]' [Locations]</li>
		
        <li>name->[MantisNames] '[negative]' [Races]</li>		
		<li>name->[MantisNames] '[Locations]' [Gangsta]</li>
		<li>name->[MantisNames] '[color]' [Locations]</li>
		
		<li>name->[Adjective] '[Locations]' [MantisNames]</li>
		<li>name->[color] '[Locations]' [Gangsta]</li>
        <li>name->[color] '[Gangsta]' [Locations]</li>
		<li>name->[Adjective] '[color]' [MantisNames]</li>
		


		</rulesStrings>
	  
	<rulesRaw>
		<li Class="Rule_File">
          <keyword>tribalword</keyword>
          <path>Words/Foreign/Tribal</path>
        </li>
	
		<li Class="Rule_File">
          <keyword>badassanimal</keyword>
          <path>Words/Nouns/Animals_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>animal</keyword>
          <path>Words/Nouns/Animals</path>
        </li>
        <li Class="Rule_File">
          <keyword>badass</keyword>
          <path>Words/Nouns/People_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>games</keyword>
          <path>Words/Nouns/Games</path>
        </li>
        <li Class="Rule_File">
          <keyword>negative</keyword>
          <path>Words/Nouns/Concepts_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>color</keyword>
          <path>Words/Nouns/Colors</path>
        </li>
		<li Class="Rule_File">
          <keyword>Adjective</keyword>
          <path>MantisNames/MantisAdj</path>
        </li>
		<li Class="Rule_File">
          <keyword>Locations</keyword>
          <path>MantisNames/MantisLocations</path>
        </li>
		<!-- noun -->
		<li Class="Rule_File">
          <keyword>Names</keyword>
          <path>MantisNames/MantisNames</path>
        </li>
		<li Class="Rule_File">
          <keyword>Gangsta</keyword>
          <path>MantisNames/MantisGangsta</path>
        </li>
		<li Class="Rule_File">
          <keyword>Races</keyword>
          <path>MantisNames/MantisRaces</path>
        </li>
	</rulesRaw>
	  
    </rulePack>
  </RulePackDef>
  
</Defs>