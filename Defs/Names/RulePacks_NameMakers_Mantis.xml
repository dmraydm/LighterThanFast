<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerPersonMantis</defName>
    <rulePack>
		<rulesStrings>
        <li>name->[badass] '[MantisAdj]' [animal]</li>
        <li>name->[negative] '[color]' [badassanimal]</li>
        <li>name->[MantisAdj] '[badass]' [roman] </li>
        <li>name->[negative] '[MantisAdj]' [badassanimal]</li>
		<li>name->[roman] '[MantisAdj2]' [MantisNames2]  </li>
        <li>name->[roman] '[color]' [MantisNames]</li>
		<li>name->[roman] '[MantisAdj]' [MantisNames]</li>
		<li>name->[color] '[MantisAdj2]' [MantisNames2]</li>
		<li>name->[negative] '[MantisAdj2]' [MantisNames]</li>
        <li>name->[color] '[MantisNames2]' [roman]</li>
        <li>name->[roman] '[MantisAdj2]' [badassanimal] </li>
		</rulesStrings>
	  
	<rulesRaw>
		<li Class="Rule_File">
          <keyword>badassanimal</keyword>
          <path>Words/Nouns/Animals_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>animal</keyword>
          <path>Words/Nouns/Animals</path>
        </li>
        <li Class="Rule_File">
          <keyword>badass</keyword>
          <path>Words/Nouns/People_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>games</keyword>
          <path>Words/Nouns/Games</path>
        </li>
        <li Class="Rule_File">
          <keyword>negative</keyword>
          <path>Words/Nouns/Concepts_Badass</path>
        </li>
        <li Class="Rule_File">
          <keyword>roman</keyword>
          <path>Words/Misc/Numbers_SmallRoman</path>
        </li>
        <li Class="Rule_File">
          <keyword>color</keyword>
          <path>Words/Nouns/Colors</path>
        </li>
		<li Class="Rule_File">
          <keyword>MantisAdj</keyword>
          <path>MantisNames/MantisAdj</path>
        </li>
		<li Class="Rule_File">
          <keyword>MantisAdj2</keyword>
          <path>MantisNames/MantisAdj2</path>
        </li>
		<!-- noun -->
		<li Class="Rule_File">
          <keyword>MantisNames</keyword>
          <path>MantisNames/MantisNames</path>
        </li>
		<li Class="Rule_File">
          <keyword>MantisNames2</keyword>
          <path>MantisNames/MantisNames2</path>
        </li>
