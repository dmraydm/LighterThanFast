<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <!-- Tribe -->

  <RulePackDef>
    <defName>NamerFactionZoltanVillage</defName>
    <rulePack>
      <rulesStrings>
        <li>name->The [illusion] [terrainfeature] [wavyName]</li>
        <li>name->[shining] [wavyAdj] [wavyName] </li>
        <li>name->[color] [virtuous] [terrainfeature]</li>
        <li>name->The [transparent] [wavyAdj] [wavyName]</li>
        <li>name->[color] [virtuous] [wavyAdj] [terrainfeature]</li>
		<li>name->[color] [shining] [politicalunion]</li>
		<li>name->[virtuous] [terrainfeature] [politicalunion]</li>
		<li>name->[illusion] [wavyAdj] [politicalunion]</li>
		<li>name->[virtuous] [wavyName] [politicalunion]</li>
      </rulesStrings>
      <rulesRaw>
		<li Class="Rule_File">
          <keyword>politicalunion</keyword>
          <path>Words/Nouns/PoliticalUnions_Outlander</path>
        </li>
        <li Class="Rule_File">
          <keyword>terrainfeature</keyword>
          <path>Words/Nouns/TerrainFeatures</path>
        </li>
		
		
		<li Class="Rule_File">
          <keyword>wavyAdj</keyword>
          <path>ZoltanNames/wavyAdj</path>
        </li>
		<li Class="Rule_File">
          <keyword>wavyName</keyword>
          <path>ZoltanNames/wavyName</path>
        </li>
		<!-- adj -->
		<li Class="Rule_File">
          <keyword>transparent</keyword>
          <path>ZoltanNames/transparent</path>
        </li>
		<li Class="Rule_File">
          <keyword>illusion</keyword>
          <path>ZoltanNames/illusion</path>
        </li>
		<li Class="Rule_File">
          <keyword>virtuous</keyword>
          <path>ZoltanNames/virtuous</path>
        </li>
		<li Class="Rule_File">
          <keyword>shining</keyword>
          <path>ZoltanNames/shining</path>
        </li>
		<li Class="Rule_File">
          <keyword>color</keyword>
          <path>Words/Nouns/Colors</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerBaseZoltanVillage</defName>
    <rulePack>
      <rulesStrings>
        <li>name->The [shining] [terrainfeature] [wavyName]</li>
        <li>name->[color] [illusion] [wavyName] </li>
        <li>name->[wavyAdj] [virtuous] [terrainfeature]</li>
        <li>name->The [wavyAdj] [illusion] [wavyName]</li>
        <li>name->[virtuous] [wavyAdj] [terrainfeature]</li>
		<li>name->[color] [shining] [wavyName]</li>
		<li>name->[transparent] [terrainfeature] [wavyName]</li>
		<li>name->[shining] [wavyAdj] [wavyName]</li>
		<li>name->[virtuous] [illusion] [wavyName]</li>
      </rulesStrings>
      <rulesRaw>
		
        <li Class="Rule_File">
          <keyword>terrainfeature</keyword>
          <path>Words/Nouns/TerrainFeatures</path>
        </li>
		<li Class="Rule_File">
			<keyword>illusion</keyword>
			<path>ZoltanNames/illusion</path>
        </li>
		
        <li Class="Rule_File">
          <keyword>positive</keyword>
          <path>Words/Nouns/Concepts_Positive</path>
        </li>
		<li Class="Rule_File">
          <keyword>wavyAdj</keyword>
          <path>ZoltanNames/wavyAdj</path>
        </li>
		<li Class="Rule_File">
          <keyword>wavyName</keyword>
          <path>ZoltanNames/wavyName</path>
        </li>
		<li Class="Rule_File">
          <keyword>virtuous</keyword>
          <path>ZoltanNames/virtuous</path>
        </li>
		<li Class="Rule_File">
          <keyword>shining</keyword>
          <path>ZoltanNames/shining</path>
        </li>
		<li Class="Rule_File">
          <keyword>color</keyword>
          <path>Words/Nouns/Colors</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

</Defs>
