<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <!-- Tribe -->

  <RulePackDef>
    <defName>NamerFactionMantisVillage</defName>
    <rulePack>
      <rulesStrings>
        <li>name->The [color] [politicalunion] for [Locations]</li>
        <li>name->[color] [negative] [Locations] </li>
        <li>name->[negative] [Adjective] [terrainfeature]</li>
        <li>name->The [Adjective] [terrainfeature] [Locations]</li>
        <li>name->The [color] [terrainfeature] [Locations]</li>
		<li>name->[Adjective] [Locations] [politicalunion]</li>
		<li>name->[Adjective] [terrainfeature] [politicalunion]</li>
		<li>name->[Adjective] [Locations] [politicalunion]</li>
		<li>name->[Adjective] [Races] [politicalunion]</li>
      </rulesStrings>
      <rulesRaw>
		<li Class="Rule_File">
          <keyword>politicalunion</keyword>
          <path>Words/Nouns/PoliticalUnions_Outlander</path>
        </li>
        <li Class="Rule_File">
          <keyword>terrainfeature</keyword>
          <path>Words/Nouns/TerrainFeatures</path>
        </li>
		<!-- adj -->
		<li Class="Rule_File">
          <keyword>color</keyword>
          <path>Words/Nouns/Colors</path>
        </li>
        <li Class="Rule_File">
          <keyword>negative</keyword>
          <path>Words/Nouns/Concepts_Badass</path>
        </li>
		<li Class="Rule_File">
          <keyword>Adjective</keyword>
          <path>MantisNames/MantisAdj</path>
        </li>

		<!-- noun -->
		<li Class="Rule_File">
          <keyword>Locations</keyword>
          <path>MantisNames/MantisLocations</path>
        </li>
		<li Class="Rule_File">
          <keyword>Names</keyword>
          <path>MantisNames/MantisNames</path>
        </li>
		<li Class="Rule_File">
          <keyword>Gangsta</keyword>
          <path>MantisNames/MantisGangsta</path>
        </li>
		<li Class="Rule_File">
          <keyword>Races</keyword>
          <path>MantisNames/MantisRaces</path>
        </li>
		
      </rulesRaw>
    </rulePack>
  </RulePackDef>

