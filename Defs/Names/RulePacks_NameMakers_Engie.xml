<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerPersonEngie</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[color] '[moreit]' [animal]</li>
        <li>name->[scifi] '[moreit]' [animal]</li>
        <li>name->[scifi] '[it]' [version]</li>
		
        <li>name->[moreit] '[it]' [animal]</li>
        <li>name->[moreit] '[scifi]' [engie]</li>
		<li>name->[moreit] '[it]' [engie]</li>
		
		<li>name->[version] '[games]' [moreit]</li>
		
        <li>name->[workPlaces] '[engie]' [animal]</li>
        <li>name->[color] '[it]' [version]</li>
		<li>name->[workPlaces] '[engie]' [moreit]</li>		
		
		<li>name->[moreit] '[sci]' [engie]</li>
		<li>name->[color] '[scifi]' [engie]</li>
		<li>name->[workPlaces] '[version]' [engie]</li>
		
		<li>name->[it] '[animal]' [engie]</li>
		<li>name->[color] '[animal]' [engie]</li>
		
		<li>name->[scifi] [workPlaces]</li>
      </rulesStrings>

      <rulesRaw>
		<li Class="Rule_File">
          <keyword>workPlaces</keyword>
          <path>EngieNames/workPlaces</path>
        </li>
	  
		<li Class="Rule_File">
          <keyword>tribalword</keyword>
          <path>Words/Foreign/Tribal</path>
        </li>
		
        <li Class="Rule_File">
          <keyword>engie</keyword>
          <path>EngieNames/Engie</path>
        </li>
        <li Class="Rule_File">
          <keyword>moreit</keyword>
          <path>EngieNames/MoreIt</path>
        </li>
        <li Class="Rule_File">
          <keyword>scifi</keyword>
          <path>EngieNames/Scifi</path>
        </li>
        <li Class="Rule_File">
          <keyword>it</keyword>
          <path>EngieNames/It</path>
        </li>
		<li Class="Rule_File">
          <keyword>version</keyword>
          <path>EngieNames/Version</path>
        </li>
		<li Class="Rule_File">
          <keyword>animal</keyword>
          <path>Words/Nouns/Animals</path>
        </li>
        <li Class="Rule_File">
          <keyword>games</keyword>
          <path>Words/Nouns/Games</path>
        </li>

        <li Class="Rule_File">
          <keyword>color</keyword>
          <path>Words/Nouns/Colors</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  
</Defs>