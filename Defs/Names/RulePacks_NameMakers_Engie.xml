<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerPersonEngie</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[color] '[animal]' [moreit]</li>
        <li>name->[moreit] '[color]' [games]</li>
        <li>name->[engie] '[moreit]' [it]</li>
        <li>name->[scifi] '[games]' [version]</li>
        <li>name->[engie] '[it]' [animal]</li>
        <li>name->[engie] '[scifi]' [animal]</li>
      </rulesStrings>

      <rulesRaw>
        <li Class="Rule_File">
          <keyword>engie</keyword>
          <path>EngieNames/Engie</path>
        </li>
        <li Class="Rule_File">
          <keyword>moreit</keyword>
          <path>EngieNames/MoreIt</path>
        </li>
        <li Class="Rule_File">
          <keyword>scifi</keyword>
          <path>EngieNames/Scifi</path>
        </li>
        <li Class="Rule_File">
          <keyword>it</keyword>
          <path>EngieNames/It</path>
        </li>
		<li Class="Rule_File">
          <keyword>version</keyword>
          <path>EngieNames/Version</path>
        </li>
		<li Class="Rule_File">
          <keyword>animal</keyword>
          <path>Words/Nouns/Animals</path>
        </li>
        <li Class="Rule_File">
          <keyword>games</keyword>
          <path>Words/Nouns/Games</path>
        </li>
		<!--
        <li Class="Rule_File">
          <keyword>roman</keyword>
          <path>Words/Misc/Numbers_SmallRoman</path>
        </li>
		-->
        <li Class="Rule_File">
          <keyword>color</keyword>
          <path>Words/Nouns/Colors</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  
</Defs>