<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerPersonEngie</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[color] '[moreit]' [OpSys]</li>
        <li>name->[color] '[it]' [versionNum]</li>
		<li>name->[color] '[engie]' [alpha]</li>
		
        <li>name->[scifi] '[alpha]' [versionName]</li>
        <li>name->[scifi] '[it]' [alpha]</li>
		
        <li>name->[OpSys] '[versionName]' [animal]</li>
		<li>name->[OpSys] '[versionName]' [versionNum]</li>
		<li>name->[OpSys] '[alpha]' [versionNum] </li>

		<li>name->[moreit] '[OpSys]' [alpha]</li>
        
		<li>name->[versionNum] '[OpSys]' [engie]</li>
		<li>name->[it] '[alpha]' [engie]</li>
		
		<li>name->[alpha] '[games]' [versionNum]</li>
		<li>name->[workPlaces] '[engie]' [moreit]</li>		
        
		
		<li>name->[alpha] '[scifi]' [versionNum]</li>
		<li>name->[alpha] '[color]' [engie]</li>
		<li>name->[alpha] '[color]' [versionNum]</li>
		<li>name->[alpha] '[versionName]' [versionNum]</li>
		
      </rulesStrings>

      <rulesRaw>
		<li Class="Rule_File">
          <keyword>workPlaces</keyword>
          <path>EngieNames/workPlaces</path>
        </li>
	  
		<li Class="Rule_File">
          <keyword>tribalword</keyword>
          <path>Words/Foreign/Tribal</path>
        </li>
		
		
        <li Class="Rule_File">
          <keyword>engie</keyword>
          <path>EngieNames/Engie</path>
        </li>
        <li Class="Rule_File">
          <keyword>moreit</keyword>
          <path>EngieNames/MoreIt</path>
        </li>
        <li Class="Rule_File">
          <keyword>scifi</keyword>
          <path>EngieNames/Scifi</path>
        </li>
        <li Class="Rule_File">
          <keyword>it</keyword>
          <path>EngieNames/It</path>
		</li>
		
		<li Class="Rule_File">
          <keyword>OpSys</keyword>
          <path>EngieNames/OperatingSystems</path>
        </li>
		
		<li Class="Rule_File">
          <keyword>alpha</keyword>
          <path>Common/GreekAlphabet</path>
        </li>
		
		<li Class="Rule_File">
          <keyword>versionNum</keyword>
          <path>EngieNames/VersionNum</path>
        </li>
		<li Class="Rule_File">
          <keyword>versionName</keyword>
          <path>EngieNames/VersionName</path>
        </li>
		
		<li Class="Rule_File">
          <keyword>animal</keyword>
          <path>Words/Nouns/Animals</path>
        </li>
        <li Class="Rule_File">
          <keyword>games</keyword>
          <path>Words/Nouns/Games</path>
        </li>

        <li Class="Rule_File">
          <keyword>color</keyword>
          <path>Words/Nouns/Colors</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>
  
</Defs>