        <li>name->[color] '[badassanimal]' [stars]</li>
		<li>name->[transparent] '[alpha]' [animal]</li>
        <li>name->[virtuous] '[stars]' [badassanimal]</li>
		
        <li>name->[virtuous] '[stars]' [wavyName]</li>
        <li>name->[transparent] '[stars]' [alpha]</li>
		<li>name->[shining] '[stars]' [wavyName]</li>
		
        <li>name->[alpha] '[wavyName]' [stars]</li>
		
		<li>name->[transparent] '[wavyAdj]' [alpha]</li>
		
		<li>name->[stars] '[illusion]' [wavyName]</li>
		<li>name->[wavyName] '[shining]' [stars]</li>
		
		<li>name->[wavyAdj] '[stars]' [alpha]</li>
		<li>name->[illusion] '[wavyAdj]' [stars]</li>
		
		<li>name->[virtuous] '[alpha]' [wavyName]</li>
		<li>name->[transparent] '[color]' [wavyName]</li>

		</rulesStrings>
	  
	<rulesRaw>
