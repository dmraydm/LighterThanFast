<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <!-- Tribe -->

  <RulePackDef>
    <defName>NamerFactionEngieVillage</defName>
    <rulePack>
      <rulesStrings>
        <li>name->DB [it] cluster [version]</li>
        <li>name->The [scifi] [it] datacenter [version] </li>
        <li>name->Meta[moreit] [version] [politicalunion]</li>
        <li>name->Crypto[it] [version] </li>
        <li>name->[moreit] [politicalunion] of [positive] [version]</li>
		<li>name->[negative] [games] of [negative] [version]</li>
		<li>name->[positive] [politicalunion] of [negative] [version]</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>politicalunion</keyword>
          <path>Words/Nouns/PoliticalUnions_Outlander</path>
        </li>
		
        <li Class="Rule_File">
          <keyword>positive</keyword>
          <path>Words/Nouns/Concepts_Positive</path>
        </li>
		
		<li Class="Rule_File">
          <keyword>version</keyword>
          <path>EngieNames/Version</path>
        </li>
		
        <li Class="Rule_File">
          <keyword>moreit</keyword>
          <path>EngieNames/MoreIt</path>
        </li>
		
        <li Class="Rule_File">
          <keyword>scifi</keyword>
          <path>EngieNames/Scifi</path>
        </li>
		
        <li Class="Rule_File">
          <keyword>games</keyword>
          <path>Words/Nouns/Games</path>
        </li>
        <li Class="Rule_File">
          <keyword>negative</keyword>
          <path>Words/Nouns/Concepts_Badass</path>
        </li>
		
        <li Class="Rule_File">
          <keyword>it</keyword>
          <path>EngieNames/It</path>
		</li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerBaseEngieVillage</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[moreit] [version] datacenter</li>
        <li>name->[scifi] retro[moreit] HA cluster</li>
        <li>name->[scifi] [it]nextGen parts</li>
        <li>name->[scifi] [it] [version] series</li>
        <li>name->[moreit] post[version] quantic dream</li>
      </rulesStrings>
	  
	  <rulesRaw>
		<li Class="Rule_File">
          <keyword>version</keyword>
          <path>EngieNames/Version</path>
        </li>
        <li Class="Rule_File">
          <keyword>moreit</keyword>
          <path>EngieNames/MoreIt</path>
        </li>
        <li Class="Rule_File">
          <keyword>scifi</keyword>
          <path>EngieNames/Scifi</path>
        </li>
        <li Class="Rule_File">
          <keyword>it</keyword>
          <path>EngieNames/It</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

</Defs>
