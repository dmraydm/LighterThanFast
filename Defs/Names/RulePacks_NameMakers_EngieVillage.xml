<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <!-- Tribe -->

  <RulePackDef>
    <defName>NamerFactionEngieVillage</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[color] [it] [workPlaces] </li>
        <li>name->The [scifi] [terrainfeature] [version] </li>
		<li>name->The [positive] [terrainfeature] engi [type] [workPlaces] </li>
        <li>name->[it] [workPlaces] [politicalunion]</li>
        <li>name->[terrainfeature] for [moreit] [version]  </li>
		<li>name->[moreit] [workPlaces] [terrainfeature]</li>
        <li>name->[politicalunion] [type] of [moreit]</li>
		<li>name->[negative] [workPlaces] of [moreit]</li>
		<li>name->[positive] [politicalunion] of [it]</li>
		
		<li>name->[lightVessel] [type] [vesselRelated]</li>
		<li>name->[mediumVessel] [type] [terrainfeature]</li>
		<li>name->[mediumVessel] [type] [vesselRelated] [politicalunion]</li>
      </rulesStrings>
	  
	  <rulesRaw>
		 <li Class="Rule_File">
          <keyword>lightVessel</keyword>
          <path>Vessels/light</path>
        </li>
		<li Class="Rule_File">
          <keyword>mediumVessel</keyword>
          <path>Vessels/medium</path>
        </li>
		<li Class="Rule_File">
          <keyword>heavyVessel</keyword>
          <path>Vessels/heavy</path>
        </li>
		<li Class="Rule_File">
          <keyword>type</keyword>
          <path>Vessels/type</path>
        </li>
		<li Class="Rule_File">
          <keyword>vesselRelated</keyword>
          <path>Vessels/vesselRelated</path>
        </li>
		
		<li Class="Rule_File">
          <keyword>workPlaces</keyword>
          <path>EngieNames/workPlaces</path>
        </li>
		
		 <li Class="Rule_File">
          <keyword>terrainfeature</keyword>
          <path>Words/Nouns/TerrainFeatures</path>
        </li>
        <li Class="Rule_File">
          <keyword>politicalunion</keyword>
          <path>Words/Nouns/PoliticalUnions_Outlander</path>
        </li>
		
        <li Class="Rule_File">
          <keyword>positive</keyword>
          <path>Words/Nouns/Concepts_Positive</path>
        </li>
		
		<li Class="Rule_File">
          <keyword>version</keyword>
          <path>EngieNames/Version</path>
        </li>
		
        <li Class="Rule_File">
          <keyword>moreit</keyword>
          <path>EngieNames/MoreIt</path>
        </li>
		
        <li Class="Rule_File">
          <keyword>scifi</keyword>
          <path>EngieNames/Scifi</path>
        </li>
		
        <li Class="Rule_File">
          <keyword>games</keyword>
          <path>Words/Nouns/Games</path>
        </li>
        <li Class="Rule_File">
          <keyword>negative</keyword>
          <path>Words/Nouns/Concepts_Badass</path>
        </li>
		
        <li Class="Rule_File">
          <keyword>it</keyword>
          <path>EngieNames/It</path>
		</li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerBaseEngieVillage</defName>
