<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <!-- Tribe -->

  <RulePackDef>
    <defName>NamerFactionEngieVillage</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[color] [it] [moreit] </li>
        <li>name->The [scifi] [terrainfeature] [version] </li>
		<li>name->The [positive] [terrainfeature] [version] </li>
        <li>name->[it] [version] [politicalunion]</li>
        <li>name->[terrainfeature] for [moreit][version]  </li>
		<li>name->[moreit] [version] [terrainfeature]</li>
        <li>name->[moreit] [politicalunion] of [moreit]</li>
		<li>name->[negative] [version] of [moreit]</li>
		<li>name->[positive] [politicalunion] of [it]</li>
      </rulesStrings>
      <rulesRaw>
		 <li Class="Rule_File">
          <keyword>terrainfeature</keyword>
          <path>Words/Nouns/TerrainFeatures</path>
        </li>
        <li Class="Rule_File">
          <keyword>politicalunion</keyword>
          <path>Words/Nouns/PoliticalUnions_Outlander</path>
        </li>
		
        <li Class="Rule_File">
          <keyword>positive</keyword>
          <path>Words/Nouns/Concepts_Positive</path>
        </li>
		
		<li Class="Rule_File">
          <keyword>version</keyword>
          <path>EngieNames/Version</path>
        </li>
		
        <li Class="Rule_File">
          <keyword>moreit</keyword>
          <path>EngieNames/MoreIt</path>
        </li>
		
        <li Class="Rule_File">
          <keyword>scifi</keyword>
          <path>EngieNames/Scifi</path>
        </li>
		
        <li Class="Rule_File">
          <keyword>games</keyword>
          <path>Words/Nouns/Games</path>
        </li>
        <li Class="Rule_File">
          <keyword>negative</keyword>
          <path>Words/Nouns/Concepts_Badass</path>
        </li>
		
        <li Class="Rule_File">
          <keyword>it</keyword>
          <path>EngieNames/It</path>
		</li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>NamerBaseEngieVillage</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[terrainfeature] [version] [moreit]</li>
        <li>name->[scifi] [moreit] [terrainfeature]</li>
        <li>name->[scifi] [color] [terrainfeature]</li>
        <li>name->[it] [version] [terrainfeature]</li>
        <li>name->[color] [terrainfeature] [moreit]</li>
      </rulesStrings>
	  
	  <rulesRaw>
		 <li Class="Rule_File">
          <keyword>terrainfeature</keyword>
          <path>Words/Nouns/TerrainFeatures</path>
        </li>
        <li Class="Rule_File">
          <keyword>politicalunion</keyword>
          <path>Words/Nouns/PoliticalUnions_Outlander</path>
        </li>
		<li Class="Rule_File">
          <keyword>version</keyword>
          <path>EngieNames/Version</path>
        </li>
        <li Class="Rule_File">
          <keyword>moreit</keyword>
          <path>EngieNames/MoreIt</path>
        </li>
        <li Class="Rule_File">
          <keyword>scifi</keyword>
          <path>EngieNames/Scifi</path>
        </li>
        <li Class="Rule_File">
          <keyword>it</keyword>
          <path>EngieNames/It</path>
        </li>
		<li Class="Rule_File">
          <keyword>color</keyword>
          <path>Words/Nouns/Colors</path>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>

</Defs>
