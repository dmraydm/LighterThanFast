        <li>name->[version] [moreit] [terrainfeature]</li>
        <li>name->[scifi] [moreit] [workPlaces]</li>
        <li>name->[color] [moreit] [terrainfeature]</li>
        <li>name->[it] [version] [terrainfeature]</li>
        <li>name->[moreit] [version] [workPlaces]</li>
		
		<li>name->[terrainfeature] [version] [workPlaces]</li>
        <li>name->[scifi] [terrainfeature] [workPlaces]</li>
        <li>name->[workPlaces] [version] [terrainfeature]</li>
        <li>name->[color] [terrainfeature] [workPlaces]</li>
		
		<li>name->[color] [version] [workPlaces]</li>
		
		<li>name->[lightVessel] [type] [vesselRelated]</li>
		<li>name->[mediumVessel] [type] [terrainfeature]</li>
		<li>name->[mediumVessel] [vesselRelated] [politicalunion]</li>
