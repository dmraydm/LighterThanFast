<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <!-- Tribe -->

  <RulePackDef>
    <defName>NamerFactionEngieVillage</defName>
    <rulePack>
      <rulesStrings>
        <li>name->[color] [it] [workPlaces]</li>
		<li>name->[it] [scifi] [workPlaces]</li>
		<li>name->The [positive] [terrainfeature] engi [type] [workPlaces]</li>
		<li>name->[negative] [workPlaces] of [moreit]</li>
		<li>name->[vesselRelated] [lightVessel] [workPlaces]</li>
		<li>name->[OpSys] [versionName] [workPlaces]</li>
		
		<li>name->[it] [scifi] [terrainfeature]</li>
        <li>name->The [scifi] [terrainfeature] [[versionNum]</li>
		<li>name->[mediumVessel] [type] [terrainfeature]</li>
        <li>name->[scifi] [workPlaces] [terrainfeature]</li>
		<li>name->[moreit] [workPlaces] [terrainfeature]</li>
		
		<li>name->[alpha] [moreit] [terrainfeature]</li>
		<li>name->[scifi] [terrainfeature] [versionNum]</li>
		
		
        <li>name->[politicalunion] [type] of [moreit]</li>
		<li>name->[politicalunion] [alpha] of [it]</li>
		
		
		
		<li>name->[it] [workPlaces] [politicalunion]</li>
		<li>name->[mediumVessel] [alpha] [politicalunion]</li>
      </rulesStrings>
	  
	  <rulesRaw>
		 <li Class="Rule_File">
          <keyword>lightVessel</keyword>
          <path>Vessels/light</path>
        </li>
		<li Class="Rule_File">
          <keyword>mediumVessel</keyword>
